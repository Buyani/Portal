<!doctype html>
<html lang="en" class="fullscreen-bg">

<head>
    <title>-10 trades</title>
    <link rel="shortcut icon" type="image/jpg" href="~/first/img/favicon.ico" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <!-- VENDOR CSS -->
    <link rel="stylesheet" href="~/first/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/first/vendor/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/first/vendor/linearicons/style.css">
    <!-- MAIN CSS -->
    <link rel="stylesheet" href="~/first/css/main.css">
    <!-- FOR DEMO PURPOSES ONLY. You should remove this in your project -->
    <link rel="stylesheet" href="~/first/css/demo.css">
    <!-- GOOGLE FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700" rel="stylesheet">
    <!-- ICONS -->
    <link rel="apple-touch-icon" sizes="76x76" href="~/first/img/apple-icon.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/theme/first/img/user.png">

</head>
<body>
    <div id="wrapper">
        <div class="vertical-align-wrap">
            @if (TempData["error"] != null)
            {
                <span class="alert alert-danger">@TempData["error"]</span>
            }
            <div class="vertical-align-middle">
                <div class="auth-box ">
                    <div class="left">

                        @RenderBody()
                    </div>
                    <div class="right">
                        <div class="overlay"></div>
                        <div class="content text">
                            <h1 class="heading">10 trades</h1>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
<style type="text/css">
    .auth-box .right {
        float: right;
        width: 58%;
        height: 100%;
        position: relative;
        background-image: url('../../first/img/first.jpg');
        background-repeat: no-repeat;
        background-size: cover;
    }
</style>











